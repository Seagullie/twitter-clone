<p id="notice" class="blue-text"> <%= notice %></p>

<section>
  <div class="container wide-container row">
    <% if not user_signed_in? and false%>
      <%= render 'tweeets/trends' %>
    <% else %>
      <%= render 'tweeets/profile-analytics', user: @user %>
    <% end %>
    <%= render 'tweeets/feed', tweeets: @tweeets  %>
    <%= render 'tweeets/who_to_follow' %>
  </div>
</section>